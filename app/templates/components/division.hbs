<@layers.geoJSON @geoJSON={{@feature}} @fillColor="url(#{{this.partiesFill}})" as |layer|>
  <layer.popup>
    <strong>{{@feature.properties.ED}}</strong>
    <br>
    <br>
    {{#each-in this.candidates as |party name|}}
      {{party}}: {{name}}<br>
    {{/each-in}}
  </layer.popup>
</@layers.geoJSON>
