<div class="map-container">
  <LeafletMap @lat="50.171813" @lng="-98.301935" @zoom="10" as |layers|>
    <layers.tile @url="https://{s}.basemaps.cartocdn.com/light_all/{z}/{x}/{y}.png"/>

    {{#each this.model.features as |feature|}}
      <Division @layers={{layers}} @feature={{feature}} />
    {{/each}}
  </LeafletMap>
</div>

<svg>
  {{#each this.patternIds as |patternId|}}
    <Pattern @patternId={{patternId}} />
  {{/each}}
</svg>
{{outlet}}

